           <param name="map_jL5S1_roty">(jL5S1_roty, XsensSuit::vSJoint::jL5S1, 1)</param>
           <param name="map_jL5S1_rotz">(jL5S1_rotz, XsensSuit::vSJoint::jL5S1, 2)</param>
